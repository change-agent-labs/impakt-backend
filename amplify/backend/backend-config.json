{
	"api": {
		"changeAgentApi": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [
						{
							"authenticationType": "API_KEY",
							"apiKeyConfig": {
								"description": "dev-key",
								"apiKeyExpirationDays": 180
							}
						}
					],
					"defaultAuthentication": {
						"authenticationType": "AMAZON_COGNITO_USER_POOLS",
						"userPoolConfig": {
							"userPoolId": "authchangeAgent"
						}
					}
				}
			}
		}
	},
	"storage": {
		"changeAgentS3Adm": {
			"service": "S3",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "changeAgentS3Trigger",
					"attributes": [
						"Name",
						"Arn",
						"LambdaExecutionRole"
					]
				}
			]
		},
		"changeAgentS3": {
			"service": "S3",
			"providerPlugin": "awscloudformation"
		},
		"changeAgentDynamo": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"auth": {
		"changeAgent": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "changeAgentCreateAuthChallenge",
					"triggerProvider": "Cognito",
					"attributes": [
						"Arn",
						"Name"
					]
				},
				{
					"category": "function",
					"resourceName": "changeAgentDefineAuthChallenge",
					"triggerProvider": "Cognito",
					"attributes": [
						"Arn",
						"Name"
					]
				},
				{
					"category": "function",
					"resourceName": "changeAgentPreSignup",
					"triggerProvider": "Cognito",
					"attributes": [
						"Arn",
						"Name"
					]
				},
				{
					"category": "function",
					"resourceName": "changeAgentVerifyAuthChallengeResponse",
					"triggerProvider": "Cognito",
					"attributes": [
						"Arn",
						"Name"
					]
				},
				{
					"category": "function",
					"resourceName": "changeAgentPostConfirmation",
					"triggerProvider": "Cognito",
					"attributes": [
						"Arn",
						"Name"
					]
				}
			]
		}
	},
	"function": {
		"changeAgentCreateAuthChallenge": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "changeAgentDynamo",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"changeAgentDefineAuthChallenge": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		},
		"changeAgentPreSignup": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "changeAgentDynamo",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"changeAgentVerifyAuthChallengeResponse": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		},
		"changeAgentPostConfirmation": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "changeAgentDynamo",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"changeAgentS3Trigger": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "api",
					"resourceName": "changeAgentApi",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				}
			]
		},
		"changeAgentDynamoTrigger": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "changeAgentDynamo",
					"attributes": [
						"Name",
						"Arn",
						"StreamArn"
					]
				},
				{
					"category": "auth",
					"resourceName": "changeAgent",
					"attributes": [
						"UserPoolId"
					]
				}
			]
		},
		"changeAgentEmailResolver": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		}
	}
}
